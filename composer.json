{
    "name": "blackbricksoftware/laravel-migration-builder",
    "description": "Programmatically create full Laravel migrations.",
    "type": "library",
    "require": {
        "php": "^8.4",
        "illuminate/support": "^12.0",
        "illuminate/filesystem": "^12.0",
        "illuminate/database": "^12.0"
    },
    "authors": [
        {
            "name": "David Hayes",
            "email": "david@blackbrick.software"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BlackBrickSoftware\\MigrationBuilder\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BlackBrickSoftware\\MigrationBuilder\\MigrationBuilderServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
